# This file is an example.
# Copy this to .env.local and fill in your actual environment variables.
# Do NOT commit .env.local to git.

# Firebase Client SDK - These are safe to be public.
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Firebase Admin SDK (Server-side)
# This is a sensitive secret. You can get this from your Firebase project settings.
# It should be a single-line JSON string.
FIREBASE_SERVICE_ACCOUNT=

# Genkit/Google AI
# API Key for Gemini Models
GEMINI_API_KEY=
